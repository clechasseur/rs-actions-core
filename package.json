{
  "name": "@clechasseur/rs-actions-core",
  "version": "7.0.3",
  "author": "clechasseur",
  "license": "MIT",
  "description": "Core functionality for the rs-* actions repos",
  "type": "module",
  "private": false,
  "main": "dist/core.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "all": "npm run refresh && npm run lint && npm test",
    "build": "tsc -p .",
    "format": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint": "oxlint",
    "refresh": "rm -rf ./dist/* && npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clechasseur/rs-actions-core.git"
  },
  "bugs": {
    "url": "https://github.com/clechasseur/rs-actions-core/issues"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@actions/cache": "^6.0.0",
    "@actions/core": "^3.0.0",
    "@actions/exec": "^3.0.0",
    "@actions/github": "^9.0.0",
    "@actions/http-client": "^4.0.0",
    "@actions/io": "^3.0.2",
    "@actions/tool-cache": "^4.0.0",
    "semver": "^7.7.4"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^24.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.14",
    "@types/semver": "^7.7.1",
    "jest": "^30.2.0",
    "oxlint": "^1.50.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
