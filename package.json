{
  "private": false,
  "name": "@clechasseur/rs-actions-core",
  "version": "5.0.2",
  "author": "clechasseur",
  "license": "MIT",
  "description": "Core functionality for the rs-* actions repos",
  "main": "dist/core.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "all": "npm run refresh && npm run lint && npm test",
    "build": "tsc -p .",
    "format": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint": "tsc --noEmit && eslint 'src/**/*.ts' '__tests__/**/*.ts'",
    "refresh": "rm -rf ./dist/* && npm run build",
    "test": "jest -c jest.config.json --runInBand",
    "watch": "tsc -p . -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clechasseur/rs-actions-core.git"
  },
  "bugs": {
    "url": "https://github.com/clechasseur/rs-actions-core/issues"
  },
  "dependencies": {
    "@actions/cache": "^4.0.1",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/http-client": "^2.2.3",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.2",
    "semver": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.17",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.5.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  }
}
