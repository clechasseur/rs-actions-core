{
  "private": false,
  "name": "@clechasseur/rs-actions-core",
  "version": "6.0.2",
  "author": "clechasseur",
  "license": "MIT",
  "description": "Core functionality for the rs-* actions repos",
  "main": "dist/core.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "all": "npm run refresh && npm run lint && npm test",
    "build": "tsc -p .",
    "format": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint": "oxlint",
    "refresh": "rm -rf ./dist/* && npm run build",
    "test": "jest -c jest.config.json --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clechasseur/rs-actions-core.git"
  },
  "bugs": {
    "url": "https://github.com/clechasseur/rs-actions-core/issues"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@actions/cache": "^4.1.0",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.1",
    "@actions/http-client": "^3.0.0",
    "@actions/io": "^2.0.0",
    "@actions/tool-cache": "^2.0.2",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/semver": "^7.7.1",
    "jest": "^30.2.0",
    "oxlint": "^1.30.0",
    "prettier": "^3.7.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}
