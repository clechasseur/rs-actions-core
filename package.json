{
  "private": false,
  "name": "@clechasseur/rs-actions-core",
  "version": "1.0.0",
  "author": "clechasseur",
  "license": "MIT",
  "description": "Core functionality for the rs-actions repos",
  "main": "dist/core.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc -p .",
    "format": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint": "tsc --noEmit && eslint 'src/**/*.ts' '__tests__/**/*.ts'",
    "refresh": "rm -rf ./dist/* && npm run build",
    "test": "jest -c jest.config.json",
    "watch": "tsc -p . -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clechasseur/rs-actions-core.git"
  },
  "bugs": {
    "url": "https://github.com/clechasseur/rs-actions-core/issues"
  },
  "dependencies": {
    "@actions/cache": "^1.0.4",
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.2",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^2.0.1",
    "@octokit/graphql": "^4.5.7",
    "@octokit/rest": "^18.0.9",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/semver": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "npm-check-updates": "^16.10.17",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  }
}