{
  "private": false,
  "name": "@clechasseur/rs-actions-core",
  "version": "2.0.0",
  "author": "clechasseur",
  "license": "MIT",
  "description": "Core functionality for the rs- actions repos",
  "main": "dist/core.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "all": "npm run refresh && npm run lint && npm test",
    "build": "tsc -p .",
    "format": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint": "tsc --noEmit && eslint 'src/**/*.ts' '__tests__/**/*.ts'",
    "refresh": "rm -rf ./dist/* && npm run build",
    "test": "jest -c jest.config.json",
    "watch": "tsc -p . -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clechasseur/rs-actions-core.git"
  },
  "bugs": {
    "url": "https://github.com/clechasseur/rs-actions-core/issues"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/cache": "^3.2.2",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1",
    "@octokit/graphql": "^5.0.6",
    "@octokit/rest": "^19.0.13",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^20.5.9",
    "@types/semver": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.9.5"
  }
}
